{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
      "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }